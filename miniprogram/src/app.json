      },
      {
        "pagePath": "pages/golist/index/index",
        "text": "去买",
        "iconPath": "./assets/icon/tab2.png",
        "selectedIconPath": "./assets/icon/tab2-active.png"
      },
      {
        "pagePath": "pages/user/index/index",
        "text": "个人",
        "iconPath": "./assets/icon/tab3.png",
        "selectedIconPath": "./assets/icon/tab3-active.png"
      }
    ]
  },
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents",
  "usingComponents": {
    "van-icon": "@vant/weapp/icon/index",
    "van-toast": "@vant/weapp/toast/index",
    "van-cell": "@vant/weapp/cell/index",
